[user]
	name = Intars Students
	email = the.mobix@gmail.com

[alias]
	co = checkout
	up = pull --rebase
	pending = !git --no-pager log origin/master..master && echo
	st = status
	ci = commit
	br = branch
	staged = diff --cached
	nst = log --oneline --date-order --graph --all --decorate
[color]
	ui = auto
[advice]
	pushNonFastForward = false
	statusHints = false
[diff]
	mnemonicprefix = true
[branch]
	autosetupmerge = true
